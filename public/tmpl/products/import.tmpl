<%= _.render('inc/head', obj) %>
<%= _.render('inc/pageTop', obj) %>
<form method="post" enctype="multipart/form-data">

	<div class="row">
		<div class="six columns">
			<label for="replaceByField">Replace product if this field is identical</label>
			<input class="u-full-width" type="text" name="replaceByField" />
		</div>

		<div class="six columns">
			<label for="file">File</label>
			<input class="u-full-width" type="file" name="file" />
		</div>
	</div>

	<div class="controls clearfix">
		<button type="submit" name="import" class="button right inv">Import</button>
		<a class="button left" href="/products/list">Cancel</a>
	</div>
</form>
<%= _.render('inc/pageBottom', obj) %>
